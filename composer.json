{
    "name": "terminal42/rokka-io-api-platform-bridge",
    "description": "Provides collection and item providers, data persisters and more to easily integrate ApiPlatform and rokka.io",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "api-platform/core": "^2.3",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "^1.0",
        "psr/http-message": "^1.0",
        "php-http/message-factory": "^1.0",
        "symfony/routing": "^4.1",
        "symfony/psr-http-message-bridge": "^1.0",
        "zendframework/zend-diactoros": "^1.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/mock-client": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\RokkaApiPlatformBridge\\": "src"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Terminal42\\RokkaApiPlatformBridge\\Test\\": "tests"
      }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yanick Witschi",
            "email": "yanick.witschi@terminal42.ch"
        }
    ]
}
